extends layout

block content
  h1= title
  p Welcome to #{title}
  form(role="form", action="/decklists/", method="post", enctype="multipart/form-data")
    div(class="form-group")
      label(for="decklist") Upload decklist
      input(type="file", name="decklist", id="decklist")
      input(type="submit", name="submit", value="Upload Decklist")